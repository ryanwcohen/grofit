<table>
  <thead>
    <tr>
      <th>
        Sculptures
      </th>
      <% @results.values.first.keys.each do |header| %>
        <th><%= header.capitalize %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @results.each do |sculpture, stats| %>
      <tr>
        <td>
          <%= sculpture %>
        </td>
        <% stats.values.each do |stat| %>
          <td style="text-align: right">
            <%= stat.to_i %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
